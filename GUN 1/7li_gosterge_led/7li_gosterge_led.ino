int a=11;
int b=10;
int c=3;
int d=4;
int e=5;
int f=9;
int g=8;


void setup() {
  pinMode(a,OUTPUT);
    pinMode(b,OUTPUT);
  pinMode(c,OUTPUT);
  pinMode(d,OUTPUT);
  pinMode(e,OUTPUT);
  pinMode(f,OUTPUT);
  pinMode(g,OUTPUT);

}

void loop() {
  //sayÄ± 1
  digitalWrite(a,HIGH);
   digitalWrite(b,LOW);
  digitalWrite(c,LOW);
    digitalWrite(d,LOW);
  digitalWrite(e,HIGH);
  digitalWrite(f,HIGH);
  digitalWrite(g,HIGH);


}
